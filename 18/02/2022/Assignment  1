assign 1
n=110
count=0
num=10
for num in range(n):
    if num<=1:
        continue
    for i in range(2,num):
        if(num%i==0):
            break
    else:
        count=count+1
        print(num)
print(count)
assign 2
n=int(input("enter n value"))
f=1
sum=0
for i in range(1,n+1):
    f=f*i
sum=sum+f
print(sum)
